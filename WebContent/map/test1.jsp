<%@ page contentType="text/html; charset=UTF-8"%>
<!DOCTYPE html>
<html>
<body>

<h1>My First Google Map</h1>

<div id="googleMap" style="width:100%;height:800px;"></div>

<script>
function myMap() {
	var latLng=new google.maps.LatLng(37.570955, 126.992413);  //위도,경도
	
	var mapProp= {
	  center:latLng, 
	  zoom:18,  //배율
	};
	var map = new google.maps.Map(document.getElementById("googleMap"),mapProp);
	
	var marker = new google.maps.Marker({
		position:latLng,  //마커 위치 지정
		animation:google.maps.Animation.BOUNCE,  //마커가 통통 튀는 효과
		icon:"https://cdn2.iconfinder.com/data/icons/icontober/64/Inkcontober_Juicy-128.png"  //마커 이미지 설정
	});
	marker.setMap(map);  //맵에 마커 등록

	new google.maps.Marker({
		position:new google.maps.LatLng(37.571064, 126.992413),  //마커 위치 지정
		animation:google.maps.Animation.BOUNCE,  //마커가 통통 튀는 효과
		icon:"https://cdn2.iconfinder.com/data/icons/icontober/64/Inkcontober_Juicy-128.png"  //마커 이미지 설정
	}).setMap(map);
	
	new google.maps.Marker({
		position:new google.maps.LatLng(37.543054, 126.992413),  //마커 위치 지정
		animation:google.maps.Animation.BOUNCE,  //마커가 통통 튀는 효과
		icon:"https://cdn2.iconfinder.com/data/icons/icontober/64/Inkcontober_Juicy-128.png"  //마커 이미지 설정
	}).setMap(map);
	
	new google.maps.Marker({
		position:new google.maps.LatLng(37.574057, 126.992413),  //마커 위치 지정
		animation:google.maps.Animation.BOUNCE,  //마커가 통통 튀는 효과
		icon:"https://cdn2.iconfinder.com/data/icons/icontober/64/Inkcontober_Juicy-128.png"  //마커 이미지 설정
	}).setMap(map);

	var infowindow = new google.maps.InfoWindow({
		  content:"이곳이 <h3 style='color:red'>맛집</br><img src='data:image/jpeg;base64,'></h3>이다"
	});

	google.maps.event.addListener(marker,'click',function() {
		map.setCenter(marker.getPosition());
		infowindow.open(map,marker);  //마커 클릭시 창 띄우기
		var pos = map.getZoom(); //기존에 설정된 줌의 중심점
		map.setZoom(15);
		
		//일정 시간 뒤에 무언가 하고 싶을 때 
		window.setTimeout(function() {map.setZoom(pos);},2400); 
	});
}
</script>
<!-- key=@@ : Google Maps API KEY 등록 -->
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDLzz2r3jR76z3wkNtlOry0MriKc2Sf3W8&callback=myMap"></script>

</body>
</html>